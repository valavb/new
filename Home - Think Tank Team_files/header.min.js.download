(function($,window){"use strict";function calcOffsetAdminBar(){var offsetAdditionalAdminBar=$("#wpadminbar").outerHeight();$(".js-header, .js-nav, .ttt-landing, .ttt-error, .ttt-cs-header").css("top",offsetAdditionalAdminBar+"px")}$(".js-menu-btn").on("click",function(e){e.preventDefault();var $this=$(this);if($this.hasClass("is-active")){$this.removeClass("is-active");$(".js-nav").removeClass("is-open");$(".js-header-overlay").removeClass("is-visible")}else{if(!$(".js-header").hasClass("is-sticky")){$("html,body").animate({scrollTop:0},"slow")}$this.addClass("is-active");$(".js-nav").addClass("is-open");$(".js-header-overlay").addClass("is-visible")}});$(".js-header-overlay").on("click",function(){$("body").removeClass("menu-opened");$(".js-menu-btn").removeClass("is-active");$(".js-nav").removeClass("is-open");$(".js-header-overlay").removeClass("is-visible")});if($(".ttt-cs-page").length){$(".js-header").addClass("is-bg")}$(window).on("load resize",function(){calcOffsetAdminBar()});window.addEventListener("orientationchange",function(){calcOffsetAdminBar()})})(jQuery,window,document);